ecmascript@0.16.9-rc300.1 # Enable ECMAScript2015+ syntax in app code
typescript@5.4.3-rc300.1 # Enable TypeScript syntax in .ts and .tsx modules
shell-server@0.6.0-rc300.1 # Server-side component of the `meteor shell` command

